@{
    ViewData["Title"] = "Home Page";
}

<h3>Web Claims:</h3>
<div class="row">
    <ul>
        @foreach (var claim in User.Claims)
        {
            <li>@claim.Type : @claim.Value</li>
        }
    </ul>
</div>

<h3>Api Claims:</h3>
<div class="row">
    <ul>
        @foreach (var value in Model)
        {
            <li>@value</li>
        }
    </ul>
</div>

